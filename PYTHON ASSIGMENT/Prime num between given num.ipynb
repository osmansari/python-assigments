   "source": [
    "n = int(input(\"Please enter a number: \"))\n",
    "prime = [] \n",
    "for num in range(2, n):\n",
    "    result = [i for i in range(2, num) if not num % i]\n",
    "    if not result:\n",
    "        prime.append(num)\n",
    "print(prime)"
